@{
	ViewData["Title"] = "My Account";
	var currentUser = await userService.CurrentUser(User);
}

@if (signInManager.IsSignedIn(User))
{
	<section id="profile-section">
		<div class="container">
			<div class="row d-flex justify-content-center">
				<h1 class="text-center mb-5 mt-2">User Profile</h1>
					<div class="col-12 col-md-5 mt-5">
						<div class="profile-img">
							@if (currentUser.ProfileImage == null)
							{
								<img src="data:image/png;base64,">

							}
							else
							{
								<img src="@currentUser.ProfileImage" alt="@currentUser.FirstName + picture" />
							}
						</div>
					</div>
					<div class="col-12 col-md-5 mt-5 user-info-container">
						<p><span class="user-span">First Name:</span> <span class="user-info-span">@currentUser.FirstName</span></p>
						<p>
							<span class="user-span">Last Name:</span> <span class="user-info-span">@currentUser.LastName</span>

						</p>
						<p>
							<span class="user-span">Email:</span> <span class="user-info-span">@currentUser.Email</span>

						</p>
						@if (currentUser.PhoneNumber != null)
						{
							<p>
								<span class="user-span">Phone Number:</span> <span class="user-info-span">@currentUser.PhoneNumber</span>

							</p>
						}
						@if (currentUser.Adresses != null)
						{
							@foreach (var adress in currentUser.Adresses)
							{
								<p>
									<span class="user-span">Street Name:</span> <span class="user-info-span">@adress.StreetName</span>

								</p>
								<p>
									<span class="user-span">Postal Code:</span> <span class="user-info-span">@adress.PostalCode</span>

								</p>
								<p>
									<span class="user-span">City:</span><span class="user-info-span"> @adress.City</span>

								</p>

							}
						}
					</div>
			</div>
		</div>
	</section>
}

